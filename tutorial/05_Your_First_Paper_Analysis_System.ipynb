  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Simple citation generator\n",
    "def generate_citation():\n",
    "    \"\"\"Generate a simple citation for the paper\"\"\"\n",
    "    \n",
    "    authors = entities.get('authors', ['Unknown Authors'])\n",
    "    institutions = entities.get('institutions', ['Unknown Institution'])\n",
    "    \n",
    "    # Simple citation format\n",
